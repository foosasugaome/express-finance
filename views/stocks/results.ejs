<div class="container-full">
    <legend>Portfolio</legend>
    <p class="text-warning"><%=message%></p>
    <table class="table table-hover">
        <thead>
        <tr>                    
            <th scope="col">Symbol</th>
            <th scope="col">Company Name</th>
            <th scope="col">Type</th>
            <th scope="col">Region</th>
            <th scope="col">&nbsp;</th>    
        </tr>
        </thead>
        <tbody>      
        <% for(i=0;i<result.bestMatches.length;i++) {%>
            <% if(result.bestMatches[i]["4. region"] == "United States") {%>
            <tr>            
                <th scope="row"><a href="/company/<%=result.bestMatches[i]["1. symbol"]%>"><%=result.bestMatches[i]["1. symbol"]%></a></th>
                <td><a href="/company/<%=result.bestMatches[i]["1. symbol"]%>"><%=result.bestMatches[i]["2. name"]%></a></td>
                <td><%=result.bestMatches[i]["3. type"]%></td>
                <td><%=result.bestMatches[i]["4. region"]%></td>
                <td><a href="/portfolio/to?symbol=<%=result.bestMatches[i]["1. symbol"]%>&name=<%=result.bestMatches[i]["2. name"]%>">➕ Portfolio</a> | <a href="/watchlist/addstock?symbol=<%=result.bestMatches[i]["1. symbol"]%>&name=<%=result.bestMatches[i]["2. name"]%>">➕ Watchlist</a></td>                
            </tr>
            <% }%>
        <%}%>
        </tbody>
    </table>
</div>