<p class="text-warning"><%=message%></p>   
<% const dollarUSLocale = new Intl.NumberFormat('en-US',{style:'currency',currency:'USD',minimumFractionDigits:2}) %> 
<div class="container-card"> 
<div class="card mb-3">
    <div class="card-body">
      <h5 class="card-title"><%=p.name%> (<%=p.ticker%>)</h5>
      <h6 class="card-subtitle text-muted"><%=p.exchange%>. Currency in <%=p.currency%></h6>
    </div>
    <h3 class="card-header"><%=parseFloat(gq["05. price"])%> 
        <small>
    <%if(gq["09. change"] < 0){%> 
        <span class="text-danger">
    <% }else{ %> 
        <span class="text-success">
    <% }%>
    <%=parseFloat(gq["09. change"])%> </span>

    <%if(parseFloat(gq["10. change percent"]) < 0){%> 
        <span class="text-danger">
    <% }else{ %> 
        <span class="text-success">
    <% }%>
    (<%=gq["10. change percent"]%>)
    </span>
    </small>
    </h3>
    <a href="/watchlist/addstock?symbol=<%=p.ticker%>&name=<%=p.name%>" class="btn btn-outline-success" style="width:fit-content; margin:5px 5px 5px 5px;"><small>‚≠ê Add to watchlist</small></a>
    <!-- <svg xmlns="http://www.w3.org/2000/svg" class="d-block user-select-none" width="100%" height="200" aria-label="Placeholder: Image cap" focusable="false" role="img" preserveAspectRatio="xMidYMid slice" viewBox="0 0 318 180" style="font-size:1.125rem;text-anchor:middle">
